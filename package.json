{
  "name": "bookstore",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/simple-server.ts",
    "start": "node dist/simple-server.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "echo \"Unit tests not implemented yet\"",
    "test:integration": "cd tests/scripts && ./runner.sh --smoke",
    "test:full": "cd tests/scripts && ./runner.sh --full",
    "test:simple": "cd tests/scripts && ./simple.sh",
    "test:health": "node tests/helpers/mongodb-health.js",
    "test:coverage": "jest --config jest.unit.config.js --coverage --passWithNoTests",
    "test:coverage:watch": "jest --config jest.unit.config.js --coverage --watch --passWithNoTests",
    "test:coverage:html": "jest --config jest.unit.config.js --coverage --passWithNoTests && open coverage/lcov-report/index.html",
    "test:coverage:demo": "jest --config jest.unit.config.js --coverage --passWithNoTests --coverageThreshold='{}'",
    "coverage": "npm run test:coverage",
    "coverage:open": "npm run test:coverage:html",
    "coverage:demo": "npm run test:coverage:demo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@effect/platform": "^0.87.7",
    "@effect/platform-node": "^0.88.11",
    "@effect/platform-node-shared": "^0.42.11",
    "@effect/schema": "^0.75.5",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "effect": "^3.16.12",
    "express": "^5.1.0",
    "mongodb": "^6.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.3",
    "c8": "^10.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nyc": "^17.1.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
